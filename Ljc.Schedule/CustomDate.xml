<?xml version="1.0" encoding="utf-8" ?>
<customdate>
  <!--要包含的日期-->
  <includedate>
    <date startdate="2017-01-30" enddate="2017-01-31"/>
    <date startdate="2017-01-30" enddate="2017-01-31"/>
    <date startdate="2017-01-30" enddate="2017-01-31"/>
    <date startdate="2017-03-31" enddate="2017-04-01"/>
  </includedate>

  <!--要排除的日期-->
  <excludedate>
    <date startdate="2017-01-31" enddate="2017-02-01"/>
    <date startdate="2017-01-31" enddate="2017-02-01"/>
    <date startdate="2017-02-10" enddate="2017-02-20"/>
    <date startdate="2017-02-10" enddate="2017-02-20"/>
  </excludedate>
</customdate>